@model List<ContatoModel>

<div class="text-center">
    <table class="table text-center" id="table-usuarios-contatos" style="width:100%;">
        <thead>
            <tr>
                <th scope="col">Id</th>
                <th scope="col">Nome</th>
                <th scope="col">Email</th>
                <th scope="col">Celular</th>
            </tr>
        </thead>
        <tbody>
            @if(Model != null && Model.Any())
            {
                foreach(ContatoModel item in Model)
                {
                    <tr>
                        <th scope="row">@item.Id</th>
                        <td>@item.Nome</td>
                        <td>@item.Email</td>
                        <td>@item.Telefone</td>
                    </tr>
                }
            }
            else
            {
                <tr>
                <tr class="odd"><td valign="top" colspan="4">Nenhum usu√°rio encontrado</td></tr>
                </tr>
            }
        </tbody>
    </table>
</div>